{
    "name": "webtrh/disqontrol",
    "description": "PHP library with full service needed for running job queues in Disque",
    "keywords": [
        "disque",
        "job",
        "task",
        "queue",
        "disqontrol"
    ],
    "homepage": "https://github.com/disqontrol/disqontrol",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schlemmer",
            "email": "lenz@centrum.cz",
            "homepage": "https://webtrh.cz/"
        },
        {
          "name": "Martin Patera",
          "email": "mzstic@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "mariano/disque-php": "^2.0@alpha",
        "symfony/config": "^2.8|^3.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/event-dispatcher": "^2.8|^3.0",
        "symfony/process": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "mtdowling/cron-expression": "^1.0",
        "psr/log": "^1.0",
        "guzzlehttp/guzzle": "^6.1",
        "doctrine/collections": "^1.3",
        "gerritdrost/phenum": "^1.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Disqontrol\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Disqontrol\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["disqontrol"]
}
