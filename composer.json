{
    "name": "disqontrol/disqontrol",
    "description": "A user-friendly framework for running background jobs in any language.",
    "keywords": [
        "disque",
        "job",
        "task",
        "queue",
        "disqontrol"
    ],
    "homepage": "https://github.com/disqontrol/disqontrol",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schlemmer",
            "email": "lenz@centrum.cz",
            "homepage": "https://disqontrol.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "mariano/disque-php": "@dev",
        "symfony/config": "^2.8|^3.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/event-dispatcher": "^2.8|^3.0",
        "symfony/expression-language": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "mtdowling/cron-expression": "^1.1",
        "psr/log": "^1.0",
        "doctrine/collections": "^1.3",
        "gerritdrost/phenum": "^1.0",
        "monolog/monolog": "^1.17",
        "webmozart/json": "^1.2",
        "vivait/backoff": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "scrutinizer/ocular": "^1.2",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Disqontrol\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Disqontrol\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/disqontrol"]
}
